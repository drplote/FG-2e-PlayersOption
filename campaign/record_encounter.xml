<root>
	<windowclass name="battle_npc" merge="join">
		<sheetdata>
			<label_encounter_itemlist name="label_items" insertbefore="maplinks_label" />
			<number_encounter_items name="items" insertbefore="maplinks_label" />

			<label_listitem_maplinks_encounter name="maplinks_label" merge="replace">
				<anchored>
					<top parent="items" anchor="bottom" offset="5" />
					<left offset="5" />
				</anchored>
			</label_listitem_maplinks_encounter>
			<list_listitem_maplinks_encounter name="maplinks" >
				<anchored>
					<top parent="items" anchor="bottom" relation="relative" offset="0" />
					<left parent="maplinks_label" anchor="right" offset="5" />
					<right offset="-5" />
				</anchored>			
			</list_listitem_maplinks_encounter>
		</sheetdata>
	</windowclass>
</root>